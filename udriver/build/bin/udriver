#!/bin/csh -f
########################################################################
#                                                                      #
# This file has been automatically generated by "gmake"                #
# using settings in reduce/src/Makefile                                #
#                                                                      #
########################################################################

cd `dirname $0`

set JAVA=java

if( $?UDRIVERPATH ) then
  set UDRIVERPATH=../lib/udriver.jar:$UDRIVERPATH
else
  set UDRIVERPATH=../lib/udriver.jar
endif

if( $#argv == 1 ) then
  $JAVA -DCONFIG_FILE=$1 -classpath ${UDRIVERPATH} warwick.marsh.ultracam.udriver.Udriver
else
  java -DCONFIG_FILE=udriver.conf -classpath ${UDRIVERPATH} warwick.marsh.ultracam.udriver.Udriver
endif

exit
